{
    "ace": {
        "@keyboardmode": "default",
        "@showFoldWidgets": false,
        "@showPrintMargin": false,
        "statusbar": {
            "@show": true
        }
    },
    "language": {
        "@continuousCompletion": false,
        "@enterCompletion": false
    },
    "tabs": {
        "@asterisk": false,
        "@show": true,
        "editorTypes": {
            "@class": "none",
            "@exe": "none",
            "@gz": "none",
            "@o": "none",
            "@pyc": "none",
            "@raw": "none",
            "@tar": "none",
            "@zip": "none"
        }
    },
    "terminal": {
        "@scrollback": 2000
    }
}